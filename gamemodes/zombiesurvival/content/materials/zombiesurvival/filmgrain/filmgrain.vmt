UnlitGeneric
{
$basetexture "zombiesurvival/filmgrain/filmgrain"
$translucent 1
$vertexcolor 1
$vertexalpha 1
$no_fullbright 1
$ignorez 1
$nolod 1

"Proxies"
	{
	"AnimatedTexture"
		{
		"animatedtexturevar" "$basetexture"
		"animatedtextureframenumvar" "$frame"
		"animatedtextureframerate" 15.00
		}
	}

}